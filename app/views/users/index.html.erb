  <h1 class='title'>Users</h1>
  <div class='topBar'>
    <% if user_signed_in? %>
      <span>Welcome, <%= current_user.name %>!</span>
      <span>
        <div>  
        <span><%= link_to "Edit Profile", edit_user_registration_path %></span>
        &nbsp;&nbsp;
        <span><%= link_to "Sign Out", destroy_user_session_path, method: :delete %></span>
        </div>
      </span>
      <!-- Display other user information -->
    <% else %>
      <span><%= link_to "Log In", new_user_session_path %></span>
      <span><%= link_to "Sign Up", new_user_registration_path %></span>
    <% end %>
  </div>
<section id='users'>
  <div class='userList'>
    <% @users.each do |user| %>
    <%= render partial: 'user', locals: { user: user } %>
    <%end%>
  </div>
</section>